#include <iostream>
using namespace std;

int x1[10], n1,;

void Afisare()
{ int i;
  for(i=1;i<=n1;i++)
    cout<<x1[i]<<" ";
  cout<<endl;}

int Valid(int k)
{
  for(int i=1;i<=k-1;i++)
     if (x1[k]==x1[i]) return 0;
  return 1;
}

void BackRec1(int k)
{
   for(int i=1;i<=n1;i++)
      { x1[k]=i;
    if (Valid(k))
       if (k==n1) Afisare();
       else BackRec1(k+1);
     }
}

int main()
{ cin>>n1;
  BackRec1(1);
  return 0;
}
